
@{
    ViewBag.Title = "Валидация тт";
    //Layout = "~/Views/Shared/_Layout.cshtml";
}
@Styles.Render("~/Content/bootstrap.css")
@Styles.Render("~/Content/bootstrap-treeview.css")
@Styles.Render("https://cdnjs.cloudflare.com/ajax/libs/jsgrid/1.5.3/jsgrid.min.css")
@Styles.Render("https://cdnjs.cloudflare.com/ajax/libs/jsgrid/1.5.3/jsgrid-theme.min.css")
<script src="http://maps.google.com/maps/api/js?key=AIzaSyDqd3og1zPoWdVVZqvUHEfZGvrLcbLmLBQ" type="text/javascript"></script>
<script src="https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/markerclusterer.js"></script>
<div class="container" >
    <h3>Валидация торговых точек</h3>
    <div class="row">
        <form>
            <div class="checkbox col-sm-4">
                <label><input type="checkbox" value="" checked="true" style="text-align:left">Только неподтвержденные</label>
            </div>
        </form>
    </div>
    
    <div class="row">
        
        <div class="col-sm-4">
            @*<h2>Link enabled, or</h2>*@
            <div id="treeview" class=""></div>
        </div>
        <div id="tbcontainer" class="pull-left col-sm-8 hidden">
            <div id="jsGrid"></div>
        </div>
    </div>
    <div class="row" id ="maprow">
        <h3><span id="mapheader" class="label label-primary "></span></h3>
        <div id="canvas" style="height: 400px; width: auto;">

        </div>
    </div>
</div>

<div id="myModal" class="modal fade" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title"><span class="label label-info">Нет идентификаций</span></h4>
            </div>
            <div class="modal-body">
                <p id ="dlgmessage">Some text in the modal.</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">OK</button>
            </div>
        </div>

    </div>
</div>

@Scripts.Render("~/Scripts/bootstrap-treeview.js")
    @*@Scripts.Render("~/Scripts/DataTables/jquery.dataTables.min.js")*@
    @Scripts.Render("https://cdnjs.cloudflare.com/ajax/libs/jsgrid/1.5.3/jsgrid.min.js")

    @*@Scripts.Render("http://maps.google.com/maps/api/js?key=AIzaSyDqd3og1zPoWdVVZqvUHEfZGvrLcbLmLBQ")
    @Scripts.Render("https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/markerclusterer.js")*@
    @Scripts.Render("~/Scripts/gsScripts/outletvalid.js")
